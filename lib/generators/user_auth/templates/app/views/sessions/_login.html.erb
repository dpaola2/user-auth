<%= form_for(:session, url: login_path, remote: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control" %>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  <%= f.submit "Log In", class: "form-control btn btn-primary" %>
<%- end %>
